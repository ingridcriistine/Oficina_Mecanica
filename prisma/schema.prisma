generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Cliente {
  id Int @id @default(autoincrement())
  name String
  email String @unique
  telefone String @unique
  endere√ßo String
  createdAt DateTime @default(now())
}

model Veiculo {
  id Int @id @default(autoincrement())
  marca String
  modelo String
  ano Int
  placa String
}

model Servico {
  id Int @id @default(autoincrement())
  descricao String
  preco Float
}